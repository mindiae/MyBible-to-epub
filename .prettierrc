{
  "useTabs": false,
  "singleQuote": true,
  "trailingComma": "none",
  "printWidth": 60,
  "tabWidth": 2,
  "plugins": [
    "prettier-plugin-svelte",
    "prettier-plugin-tailwindcss"
  ],
  "pluginSearchDirs": false,
  "overrides": [
    {
      "files": "*.svelte",
      "options": { "parser": "svelte" }
    }
  ]
}
